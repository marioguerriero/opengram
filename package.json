{
  "name": "opengram",
  "author": "Mario Guerriero",
  "repository": "git@github.com:marioguerriero/opengram.git",
  "version": "0.0.1",
  "license": "GPL-3.0",
  "private": true,
  "main": "./server/main.js",
  "scripts": {
    "start": "NODE_ENV=production node dist/main.js",
    "postinstall": "babel --presets=es2015,react server/ -d dist && next build client",
    "dev": "nodemon server/main.js --exec babel-node --presets=es2015,react",
    "build-next": "next build client",
    "build": "babel --presets=es2015,react server/ -d dist",
    "pretest": "babel --presets=es2015,react server/ -d dist && babel --presets=es2015 test/ -d test-dist",
    "test": "mocha test-dist --recursive",
    "posttest": "rm -rf test-dist"
  },
  "dependencies": {
    "autobind-decorator": "^2.1.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "express": "~4.16.2",
    "express-jwt": "^5.1.0",
    "fetch": "^1.1.0",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^8.1.1",
    "keymirror": "^0.1.1",
    "mongoose": "^5.0.2",
    "morgan": "~1.9.0",
    "multer": "^1.2.0",
    "next": "^4.2.3",
    "next-redux-wrapper": "^1.3.2",
    "node-require-jsx": "^0.1.1",
    "path-match": "^1.2.4",
    "react": "^16.2.0",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.5",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "superagent": "^3.3.1",
    "url-parse": "^1.1.3"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.1.2",
    "mocha": "^5.0.0",
    "nodemon": "^1.11.0",
    "supertest": "^3.0.0",
    "url": "^0.11.0"
  }
}
